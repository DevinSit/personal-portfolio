{
    "private": true,
    "name": "personal-portfolio",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "start": "npm run -s dev",
        "build": "preact build --template src/index.html",
        "serve": "preact build --template src/index.html && preact serve --port 3000",
        "dev": "preact watch --template src/index.html --port 3000",
        "lint": "eslint src"
    },
    "eslintConfig": {
        "extends": "eslint-config-synacor"
    },
    "eslintIgnore": [
        "build/*"
    ],
    "devDependencies": {
        "eslint": "^4.5.0",
        "eslint-config-synacor": "^1.1.0",
        "node-sass": "^4.13.1",
        "preact-cli": "^2.2.1",
        "sass-loader": "^7.1.0"
    },
    "dependencies": {
        "@analytics/google-analytics": "^0.2.2",
        "analytics": "^0.3.0",
        "classnames": "^2.2.6",
        "preact": "^10.3.1"
    }
}
